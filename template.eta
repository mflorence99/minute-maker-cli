<!doctype html>

<html lang="en">

<title>
  <%= it.subject %> - <%= it.$dayjs(it.date).format('MMMM D, YYYY') %>
</title>

<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1" />

<link href="https://fonts.googleapis.com/css2?family=Roboto&family=Roboto+Slab" rel="stylesheet">

<link href="https://fonts.googleapis.com/css2?family=Germania+One&display=swap" rel="stylesheet">

<link rel="shortcut icon" href="./minute-maker.png" />

<script src="https://unpkg.com/wavesurfer.js@7"></script>
<script src="https://unpkg.com/wavesurfer.js@7/dist/plugins/regions.min.js"></script>

<style>
  /* üü• basic structural styles for whole document */

  body {
    height: 100%;
    margin: 0;
    overflow: hidden;
    width: 100%;
  }

  button {
    background-color: <%=it.$colorMap.primary %>;
    border-radius: 0.25rem;
    border-width: 0px;
    color: <%=it.$colorMap.white %>;
    cursor: pointer;
    height: 2rem;
    padding: 0px 1rem;
    text-align: center;
    transition: background 0.125s linear;
  }

  button:hover {
    background-color: <%=it.$colorMap.primary.replace("1.0", "0.5") %>;
  }

  html {
    background-color: <%=it.$colorMap.black %>;
    height: 100%;
    width: 100%;
  }

  main {
    background-color: <%=it.$colorMap.white %>;
    color: <%=it.$colorMap.black %>;
    display: flex;
    flex-direction: column;
    font-family: "Roboto";
    font-size: 14px;
    gap: 1rem;
    height: calc(100% - 7rem);
    margin: 2rem auto;
    max-width: 44rem;
    padding: 2rem;

    /* üî• https://stackoverflow.com/questions/27074259/html-css-table-font-size-different-in-mobile-device */

    text-size-adjust: none;
    -webkit-text-size-adjust: none;
  }

  table {
    border-collapse: separate;
    border-spacing: 0;
    width: 100%;

    td {
      padding: 0.25rem;
    }

    td:first-child {
      padding-left: 0;
    }

  }

  /* üüß disclaimer table + buttons */

  #theDisclaimer {

    td {
      background-color: <%=it.$colorMap.gray15 %>;
      font-size: smaller;
      line-height: 1.25;
      padding: 0.5rem;
    }

    tr {
      vertical-align: middle;
    }

    .buttons {
      display: flex;
      flex-direction: column;
      gap: 0.5rem;
    }
  }

  /* üüß header: title, seal etc */

  #theHeader {
    align-items: center;
    display: flex;
    gap: 1rem;

    .header {
      flex-grow: 2;
      text-align: center;
    }

    .seal {
      height: clamp(5rem, 12.5vmin, 7rem);
      width: clamp(5rem, 12.5vmin, 7rem);
    }

    .subtitle {
      font-size: 1rem;
      font-weight: bold;
    }

    .title {
      font-family: "Germania One";
      font-size: clamp(1.5rem, 6vmin, 3rem);
      font-weight: bold;
    }
  }

  /* üüß  tabs: Summary, Attendees etc */

  #theTabs {
    display: flex;
    gap: 1rem;
    list-style-type: none;

    .tab {
      border: 1px solid transparent;
      color: <%=it.$colorMap.gray10 %>;
      cursor: pointer;
      transition: color 0.25s linear;
    }

    .tab.active {
      border-bottom: 1px solid <%=it.$colorMap.black %>;
      color: <%=it.$colorMap.black %>;
    }
  }

  /* üüß transcript content area: atgtendees,m summary etc */

  #theTranscript {
    flex-grow: 2;
    overflow-y: auto;

    /* üü© attendees table */

    .attendees {

      tr {
        vertical-align: top;
      }

      td:first-child {
        font-weight: bold;
        white-space: nowrap;

        &::first-letter {
          text-transform: capitalize;
        }
      }

      td:not(:first-child) {
        width: 100%;
      }
    }

    /* üü© summary table */

    .summary {
      margin-bottom: 1rem;

      ul {
        margin: 0;
      }

      li {
        margin-left: -1.25rem;
        margin-bottom: 0.25rem;
      }
    }

    /* üü© transcript table */

    .transcript {
      margin-bottom: 1rem;

      tr {
        cursor: pointer;
        vertical-align: top;
      }

      tr:hover {
        background-color: <%=it.$colorMap.gray15 %>;
      }

      tr.selected {
        background-color: <%=it.$colorMap.gray13 %>;
      }

      td:first-child {
        font-weight: bold;
        white-space: nowrap;
      }
    }

    /* ‚èπÔ∏è where all the tabbed content lies, initially invisdible */

    .tab-content {
      display: none;

      .title {
        font-weight: bold;
        font-size: larger;
      }
    }
  }

  /* üüß WaveSurfer waveform and media controls */

  #theWaveform {
    display: flex;
    flex-direction: column;
    gap: 0.5rem;

    .loading {
      align-items: center;
      display: flex;
      color: <%=it.$colorMap.black %>;
      height: 1.5rem;
      font-size: smaller;
      font-weight: bold;
      justify-content: center;
      width: 100%;

      svg {
        fill: <%=it.$colorMap.accent %>;
        flex-shrink: 0;
        height: 5rem;
        stroke: <%=it.$colorMap.accent %>;
      }
    }

    .media {
      display: none;
      height: 1.5rem;
      width: 100%;
    }

    .wavesurfer {
      background-color: transparent;

      ::part(marker) {
        background-color: <%=it.$colorMap.gray8 %>;
      }

      ::part(region-content) {
        background-color: <%=it.$colorMap.white %>;
        color: <%=it.$colorMap.gray8 %>;
        font-size: 10px;
        max-width: 25em;
        overflow: hidden;
        text-overflow: ellipsis;
        white-space: nowrap;
      }
    }
  }

  /* üü• when we print, everything (more-or-less) is displayed sequentially */

  @media print {
    main {
      height: 100%;
      margin: 0;
      max-width: 100%;
      padding: 0;
    }

    #theTabs,
    #theWaveform {
      display: none;
    }

    #theTranscript {
      flex-grow: unset;
      overflow: unset;
    }

    .tab-content {
      display: block !important;
      margin-bottom: 1rem;
    }
  }

  /* üü• on REALLY small devices ... */

  @media (width < 480px) {
    .seal {
      display: none;
    }
  }

  /* üü• on most mobile devices ... */

  @media (height < 600px) {

    #theWaveform {
      display: none;
    }
  }

  @media (height < 800px) {

    #theDisclaimer {
      display: none;
    }
  }

  @media (width < 800px) {
    main {
      height: calc(100% - 4rem);
      margin: 0;
      max-width: 100%;
    }

    #theDisclaimer {
      display: none;
    }
  }
</style>
</head>

<body>

  <main>

    <header id="theHeader">
      <article class="header">
        <div class="title"><%= it.organization %></div>
        <b class="subtitle">
          <%= it.department %>
          <br>
          <%= it.subject %> - <%= it.$dayjs(it.date).format('MMMM D, YYYY') %>
        </b>
      </article>

      <img class="seal" src="https://upload.wikimedia.org/wikipedia/en/1/1c/Sullivan_County_nh_seal.jpg">

      <img class="seal" src="https://upload.wikimedia.org/wikipedia/commons/a/aa/Seal_of_New_Hampshire.svg">

    </header>

    <% /* üü• disclaimer */ %>

    <table id="theDisclaimer">
      <tr>
        <td>
          <img width="40" src="data:image/png;base64," />
        </td>
        <td style="width: 100%">
          This transcript was produced by our
          <a href="https://github.com/mflorence99/minute-maker-cli" target="_blank">Minute Maker CLI</a>
          <a href="https://opensource.com/resources/what-open-source" target="_blank">open source software</a>, leveraging
          <a href="https://www.assemblyai.com/" target="_blank">
            AssemblyAI
          </a>
          to perform the transcription, and the
          <a href="https://platform.openai.com/docs/models" target="_blank">
            OpenAI GPT-4o
          </a>
          model to automatically generate a summaries from the transcript. Download the audio from
          <a href="<%= it.audioURL %>" target="_blank">
            this page.
          </a>
        </td>
        <td style="background: transparent">
          <article class="buttons">
            <button onClick="print()">Print</button>
          </article>
        </td>
      </tr>
    </table>

    <% /* üü• WaveSurfer waveform and media controls */ %>

    <section id="theWaveform">
      <article class="wavesurfer">
      </article>
      <article class="loading">
        Loading audio
        <svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 200 200'>
          <circle stroke-width='3' r='15' cx='40' cy='100'>
            <animate attributeName='opacity' calcMode='spline' dur='2' values='1;0;1;' keySplines='.5 0 .5 1;.5 0 .5 1' repeatCount='indefinite' begin='-.4'></animate>
          </circle>
          <circle stroke-width='3' r='15' cx='100' cy='100'>
            <animate attributeName='opacity' calcMode='spline' dur='2' values='1;0;1;' keySplines='.5 0 .5 1;.5 0 .5 1' repeatCount='indefinite' begin='-.2'></animate>
          </circle>
          <circle stroke-width='3' r='15' cx='160' cy='100'>
            <animate attributeName='opacity' calcMode='spline' dur='2' values='1;0;1;' keySplines='.5 0 .5 1;.5 0 .5 1' repeatCount='indefinite' begin='0'></animate>
          </circle>
        </svg>
      </article>
      <audio class="media" controls>
      </audio>
    </section>

    <% /* üü• tabs: Attendees etc */ %>

    <nav id="theTabs">
      <li class="tab" id="tab1" onClick="$$tab(1)">Attendees</li>
      <li class="tab" id="tab2" onClick="$$tab(2)">Summary</li>
      <li class="tab" id="tab3" onClick="$$tab(3)">Transcript</li>
    </nav>

    <% /* üü• everything tabbed fits here */ %>

    <section id="theTranscript">

      <% /* üüß attendees table */ %>

      <article class="tab-content" id="content1">
        <table class="attendees">
          <% 
            let tt = ["present", "absent", "visitors"];
            tt.forEach(function(t) {
              if (it[t].length > 0) { %>
          <tr>
            <td><%= t %></td>
            <td>
              <%= it[t].join(", ") %></td>
          </tr>
          <% }}) %>
        </table>
      </article>

      <% /* üüß summary table */ %>

      <article class="tab-content summary" id="content2">
        <% it.chapters.forEach(function(chapter) { %>
        <div class="title"><%= chapter.title %></div>
        <%~ chapter.summary %>
        <% }) %>
        <br>
      </article>

      <% /* üüß transcript table, divided into chapters */ %>

      <article class="tab-content transcript" id="content3">
        <% it.chapters.forEach(function(chapter) { %>
        <div class="title"><%= chapter.title %></div>

        <% /* üü© a new table for every chapter */ %>

        <table class="transcript">
          <% 
            for (let ix = chapter.start; ix <= chapter.end; ix++) { 
              const utterance = it.utterances[ix];
          %>
          <tr onclick="$$wavesurfer.setTime(<%= utterance.start / 1000 %>)" data-start=<%= utterance.start / 1000 %> data-end=<%= utterance.end / 1000%>>
            <td><%= it.speakers[utterance.speaker] %></td>
            <td style="font-family: monospace">
              <%= it.$dayjs(utterance.start).format('mm:ss') %>
            </td>
            <td style="width: 100%"><%= utterance.text %></td>
          </tr>
          <% } %>
        </table>
        <% }) %>
      </article>
    </section>

  </main>

  <script>
    let $$tab, $$wavesurfer;

    (function() {

      // üëá off-the-shelf binary search
      function binarySearch(array, key, comparator) {
        let lower = 0,
          upper = array.length - 1,
          mid, cmp;
        while (lower <= upper) {
          mid = Math.floor((lower + upper) / 2, 10);
          cmp = comparator(array[mid], key);
          if (cmp < 0) lower = mid + 1;
          else if (cmp > 0) upper = mid - 1;
          else return array[mid];
        }
        return null;
      }

      // üëá either manually, or via waveform position changes
      let lastSelectedRow = null;

      function selectRow(row) {
        if (!row.classList.contains("selected")) {
          row.classList.add("selected");
          row.scrollIntoView({
            behavior: "smooth",
            block: "center"
          });
          // üëâ prepare for next selectrion
          lastSelectedRow?.classList.remove("selected");
          lastSelectedRow = row;
        }
      }

      // üëá when a tab is clicked ...
      function tab(ix) {
        // üëâ hide/show the right tabs
        let children = document.querySelectorAll("#theTabs .tab");
        children.forEach((child) => {
          if (child.id === `tab${ix}`) {
            child.classList.add("active");
          } else child.classList.remove("active");
        });
        // üëâ hide/show the right content
        children = document.querySelectorAll("#theTranscript .tab-content");
        children.forEach((child) => {
          child.style.display = child.id === `content${ix}` ? "block" : "none";
        });
      }

      // üëá off-the-shelf throttle (for WaveSurfer events)
      function throttle(cb, wait) {
        var lastTime = 0;
        return function(...args) {
          var now = Date.now();
          if (now - lastTime >= wait) {
            cb(...args);
            lastTime = now;
          }
        };
      }

      // üëá create a new WaveSurfer and connect it to the DOM
      const wavesurfer = WaveSurfer.create({
        autoCenter: true,
        audioRate: 1,
        autoplay: false,
        autoScroll: true,
        backend: "WebAudio",
        barGap: 1,
        barRadius: 2,
        barWidth: 2,
        container: "#theWaveform .wavesurfer",
        cursorColor: "<%=it.$colorMap.gray8 %>",
        cursorWidth: 2,
        dragToSeek: false,
        fillParent: true,
        height: 48,
        hideScrollbar: false,
        interact: true,
        media: document.querySelector("#theWaveform .media"),
        minPxPerSec: 1,
        normalize: false,
        plugins: [WaveSurfer.Regions.create()],
        progressColor: "<%= it.$colorMap.gray14 %>",
        url: "<%= it.audioURL %>",
        waveColor: "<%= it.$colorMap.accent %>",
      });

      // üëá when it's decoded, show the chapters as markers
      wavesurfer.on("decode", () => {
        <% it.chapters.forEach(function(chapter) { %>
        wavesurfer.plugins[0].addRegion({
          drag: false,
          resize: false,
          start: <%= it.utterances[chapter.start].start / 1000 %>,
          content: "<%= chapter.title %>"
        })
        <% }) %>
      });

      // üëá when it's ready, hide the loading indicator
      wavesurfer.on("ready", () => {
        const loading = document.querySelector("#theWaveform .loading");
        loading.style.display = "none";
        const media = document.querySelector("#theWaveform .media");
        media.style.display = "block";
      });

      // üëá on throttled time changes in the waveform

      wavesurfer.on("timeupdate", throttle((currentTime) => {
        const element = binarySearch(lookup, currentTime,
          function(element, key) {
            if (key < element.start) return +1;
            if (key >= element.end) return -1;
            return 0;
          });
        if (element)
          selectRow(element.row);
      }, 1000));

      // üëá build a lookup table of all the rows of the transcription
      //    they're not going to change, so only do it once
      //    this supports the binary search when the waveform moves

      let lookup = [];

      window.onload = () => {
        const rows = Array.from(document.querySelectorAll("table.transcript tr"));
        lookup = rows.map((row) => ({
          start: Number(row.dataset.start),
          end: Number(row.dataset.end),
          row: row
        }));
      };

      // üëâ tab to the transcript initially
      tab(3);

      // üëâ poor man's "export"
      $$tab = tab;
      $$wavesurfer = wavesurfer;

    })();
  </script>

</body>

</html>
